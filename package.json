{
  "name": "@nuxtjs/auth",
  "version": "5.0.0-dev",
  "description": "Authentication module for Nuxt.js",
  "repository": "nuxt-community/auth-module",
  "license": "MIT",
  "sideEffects": "false",
  "exports": {
    ".": {
      "require": "./dist/module.js"
    },
    "./dist/module": {
      "require": "./dist/module.js"
    },
    "./dist/runtime": {
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.js"
    }
  },
  "main": "./dist/module.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "templates",
    "index.d.ts"
  ],
  "scripts": {
    "build": "siroc build && yarn build:types",
    "build:types": "./scripts/types.sh",
    "bump-next": "jiti ./scripts/bump-next",
    "dev": "nuxt-ts demo",
    "heroku-postbuild": "nuxt-ts build demo",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint-fix": "yarn lint --fix",
    "release": "yarn build && standard-version && git push --follow-tags && npm publish",
    "start": "nuxt-ts start demo",
    "test": "yarn lint && jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "consola": "^2.15.3",
    "cookie": "^0.4.1",
    "defu": "^5.0.0",
    "hasha": "^5.2.2",
    "jwt-decode": "^3.1.2",
    "requrl": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-typescript": "^7.16.5",
    "@microsoft/api-documenter": "^7.13.77",
    "@microsoft/api-extractor": "^7.19.2",
    "@nuxt/test-utils": "^0.2.2",
    "@nuxt/types": "latest",
    "@nuxt/typescript-build": "latest",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/cookie-parser": "latest",
    "@types/express-jwt": "latest",
    "@types/jest": "latest",
    "@types/jsonwebtoken": "latest",
    "@types/lodash.get": "latest",
    "@types/node": "latest",
    "@types/puppeteer": "latest",
    "babel-jest": "latest",
    "bootstrap-vue": "latest",
    "cookie-parser": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "express": "latest",
    "express-jwt": "latest",
    "jest": "^27.4.5",
    "jiti": "^1.12.9",
    "jsdom": "latest",
    "lodash.get": "latest",
    "nuxt": "^2.15.8",
    "oidc-provider": "^6.31.1",
    "playwright": "^1.17.1",
    "prettier": "latest",
    "siroc": "^0.16.0",
    "standard-version": "latest",
    "ts-jest": "latest",
    "ts-loader": "<9",
    "typescript": "latest",
    "vue": "latest"
  },
  "peerDependencies": {
    "vue": "*"
  }
}
